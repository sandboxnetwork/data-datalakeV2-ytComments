{
  "name": "sandbox-youtube-comments-lite",
  "version": "0.0.3",
  "description": "Sandbox network youtube comment analyze library",
  "main": "libs/bundle.js",
  "types": "libs/bundle.d.ts",
  "scripts": {
    "test": "ts-node ./node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "dev": "nodemon --watch src --delay 1 --exec 'ts-node' test/test.ts",
    "cpy:prod": "./node_modules/.bin/cpy ./dist/prod/bundle.d.ts ./libs/ & ./node_modules/.bin/cpy ./dist/prod/bundle.js ./libs/",
    "release": "npm run cpy:prod",
    "build:dev": "webpack --config webpack-config/webpack.dev.config.js",
    "build:prod": "webpack --config webpack-config/webpack.prod.config.js"
  },
  "keywords": [
    "youtube",
    "comment",
    "crawling",
    "extract",
    "fast"
  ],
  "author": {
    "name" : "Sandboxnetwork CT-DEV",
    "email" : "jseo@sandboxnetwork.net"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jasmine": "^3.6.9",
    "cpy-cli": "^3.1.1",
    "dts-bundle-webpack": "^1.0.2",
    "gts": "^3.1.0",
    "jasmine": "^3.7.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.19",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.27.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "form-data": "^4.0.0"
  }
}
